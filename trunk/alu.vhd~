entity alu is
	port(
			a 						:	in bit_vector(7 downto 0);
			b							: in bit_vector(7 downto 0);
			aluOperation	:	in bit_vector(1 downto 0);
			result				:	out bit_vector(7 downto 0);
			zero					: out bit;
		);
end alu;

architecture behavioral of alu is
begin
	process(a, b, aluOperation)
	begin
	end process;
end;
